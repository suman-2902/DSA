#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    std::cout<<"Hello, World!";
    return 0;
}


solution 2

#include <bits/stdc++.h>
 using namespace std;


int main() {
   int a,b,c;
   cin >>a>>b>>c;
   cout<< a+b+c<<endl; 
   
   return 0;
}

solution 3

#include <bits/stdc++.h>
using namespace std;
int main(){
int a;
long b;
char c;
float d;
double e;
cin >> a >> b >> c >> d >> e;
cout<< a << '\n' << b << '\n' << c << '\n';
cout <<fixed<<setprecision(3) << d << '\n';
cout <<fixed<<setprecision(9) << e << '\n';

return 0;
}


